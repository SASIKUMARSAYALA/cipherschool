public class Main {
    public static void main(String[] args) {
        float celsius = 30.0f;

        // Formula: Fahrenheit = (Celsius Ã— 9/5) + 32
        float fahrenheit = (celsius * 9 / 5) + 32;

        System.out.println("Celsius: " + celsius);
        System.out.println("Fahrenheit: " + fahrenheit);
    }
}
